{
  "name": "vue-leafify",
  "version": "0.1.1",
  "description": "A powerful and customizable Leaflet map component based on Vue 3",
  "author": "abdullaheraybulut",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "dist/*.css",
    "dist/*.js",
    "dist/components/*.d.ts",
    "dist/types"
  ],
  "main": "./dist/vue-leafify.umd.js",
  "module": "./dist/vue-leafify.es.js",
  "style": "./dist/style.css",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-leafify.es.js",
      "require": "./dist/vue-leafify.umd.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --mode library",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "leaflet": "^1.9.4"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.11.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "sass": "^1.69.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vue-tsc": "^2.2.4"
  },
  "keywords": [
    "vue",
    "map",
    "leaflet",
    "gis",
    "geospatial",
    "component",
    "vue3",
    "composition-api",
    "typescript"
  ]
}
